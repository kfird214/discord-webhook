{
  "author": "nisan",
  "name": "discord-webhook",
  "version": "1.0.0",
  "description": "",
  "main": "lib/webhook.js",
  "license": "ISC",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "package": "ncc build --source-map",
    "package:all": "npm run build && npm run package"
  },
  "jest-junit": {
    "outputDirectory": "reports",
    "outputName": "jest-junit.xml",
    "ancestorSeparator": " > ",
    "uniqueOutputName": "false",
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "discord.js": "^14.14.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@vercel/ncc": "^0.38.1",
    "dotenv": "^16.3.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
